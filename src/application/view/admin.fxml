<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="1100.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.AdminController">
   <children>
      <Label alignment="CENTER" layoutX="475.0" layoutY="20.0" prefWidth="150.0" text="Admin Menu">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="buttonExit" layoutX="525.0" layoutY="450.0" mnemonicParsing="false" prefWidth="50.0" style="-fx-background-color: #22b2da;" text="Exit" textFill="#fcf8f8">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TableView fx:id="tableView" layoutX="50.0" layoutY="75.0" prefHeight="350.0" prefWidth="1015.0"><!--15 extra pixels for the scroll bar-->
        <columns>
            <TableColumn fx:id="categoryColumn" minWidth="4.0" prefWidth="100.0" text="Category" />
            <TableColumn fx:id="difficultyColumn" minWidth="4.0" prefWidth="70.0" text="Difficulty" />
            <TableColumn fx:id="questionColumn" minWidth="4.0" prefWidth="230.0" text="Question" />
            <TableColumn fx:id="answerColumn" minWidth="4.0" prefWidth="150.0" text="Answer" />
            <TableColumn fx:id="incorrectAnswer1Column" minWidth="4.0" prefWidth="150.0" text="WrongAnswer1" />
            <TableColumn fx:id="incorrectAnswer2Column" minWidth="4.0" prefWidth="150.0" text="WrongAnswer2" />
            <TableColumn fx:id="incorrectAnswer3Column" minWidth="4.0" prefWidth="150.0" text="WrongAnswer3" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
